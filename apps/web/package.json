{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "codegen:init": "npx openapi-codegen init",
    "codegen:run": "npx openapi-codegen gen api"
  },
  "dependencies": {
    "@mantine/core": "7.3.2",
    "@mantine/hooks": "7.3.2",
    "@tanstack/react-query": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.0"
  },
  "devDependencies": {
    "@openapi-codegen/cli": "^2.0.0",
    "@openapi-codegen/typescript": "^8.0.0",
    "@types/react": "18.2.36",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react": "4.2.1",
    "postcss": "8.4.32",
    "postcss-preset-mantine": "1.12.0",
    "postcss-simple-vars": "7.0.1",
    "stylelint": "16.0.2",
    "vite": "5.0.10",
    "vite-tsconfig-paths": "4.2.2"
  }
}
